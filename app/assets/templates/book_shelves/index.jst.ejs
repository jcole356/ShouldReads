<h3>Bookshelves</h3>

<ul>
  <% bookShelves.each(function(bookShelf) { %>
    <li class="book-title" data-id="<%= bookShelf.id %>"><h4><%= bookShelf.escape('title') %></h4></li>
  <% }) %>
</ul>

<input class="new-shelf" type="text" name="book_shelf[title]" value="New Bookshelf Title">

<br>
<button class="btn btn-default">Add Bookshelf</button>
